@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
@Html.Action("PartialHead","Default")
<body>
    @Html.Action("PartialNavbarInfo", "Default")
    @Html.Action("PartialNavbar", "Default")
    @Html.Action("PartialSlider", "Default")
    <section class="ftco-section ftco-wrap-about ftco-no-pb ftco-no-pt">
        <div class="container">
            <div class="row no-gutters">
               
                @Html.Action("PartialRzr", "Default") 
                @Html.Action("PartialAbout", "Default")
            </div>
        </div>
    </section>

    <section class="ftco-section ftco-intro" style="background-image: url(/Templates/tasteit-master/images/bg_3.jpg);">
        <div class="overlay"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <span>Hemen Yerinizi Ayırın</span>
                    <h2>Özel Buluşmalar &amp; Happy Hours</h2>
                </div>
            </div>
        </div>
    </section>

    @Html.Action("PartialMenu", "Default")
    @Html.Action("PartialTestimonial", "Default")
    @Html.Action("PartialChef", "Default")
    @Html.Action("PartialPerfect","Default")

    <section class="ftco-section ftco-no-pt ftco-no-pb ftco-intro bg-primary">
        <div class="container py-5">
            <div class="row py-2">
                <div class="col-md-12 text-center">
                    <h2>Lezzetli &amp;  Besleyici Yemekler Yapıyoruz</h2>
                    <a href="/RezervationDetail/Index/" class="btn btn-white btn-outline-white">Şimdi Masanızı Ayırtın</a>
                </div>
            </div>
        </div>
    </section>

    <footer class="ftco-footer ftco-no-pb ftco-section">
        <div class="container">
            <div class="row mb-5">            
                
                @Html.Action("PartialTasteit","Default")              
              @Html.Action("PartialOpenHours","Default")              
             @Html.Action("PartialInstagramImage", "Default")            
               @Html.Action("PartialBulten","Default")       
            </div>
        </div>
        @Html.Action("PartialFooter","Default")
    </footer>
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>
    @Html.Action("PartialScript", "Default")
</body>
</html>